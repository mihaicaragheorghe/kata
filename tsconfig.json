{
    "compilerOptions": {
        "outDir": "dist",
        "allowJs": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "resolveJsonModule": true,
        "noImplicitReturns": true,
        "esModuleInterop": true,
        "target": "esnext",
        "module": "commonjs",
    },
    "include": [
        "./**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}